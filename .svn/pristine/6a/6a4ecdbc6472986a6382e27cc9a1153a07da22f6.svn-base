package com.frxs.PickApp.model;

public class PrintProduct
{
	private String ProductId;// "0001"//商品编号
	
	private String ItemDescription;// "王老吉"，//商品描述
	
	private String Quantity;// "3"，//购买数量
	
	private String ItemAdjustedPrice;// "10"，//单价
	
	private String Total;// "30"，//金额
	
	private int ProductType;// "0"，//0:网仓商品;1:门店商品
	
	private int VendorID;// "001"//供应商ID

	
    public String getProductId()
    {
    	return ProductId;
    }

	
    public void setProductId(String productId)
    {
    	ProductId = productId;
    }

	
    public String getItemDescription()
    {
    	return ItemDescription;
    }

	
    public void setItemDescription(String itemDescription)
    {
    	ItemDescription = itemDescription;
    }

	
    public String getQuantity()
    {
    	return Quantity;
    }

	
    public void setQuantity(String quantity)
    {
    	Quantity = quantity;
    }

	
    public String getItemAdjustedPrice()
    {
    	return ItemAdjustedPrice;
    }

	
    public void setItemAdjustedPrice(String itemAdjustedPrice)
    {
    	ItemAdjustedPrice = itemAdjustedPrice;
    }

	
    public String getTotal()
    {
    	return Total;
    }

	
    public void setTotal(String total)
    {
    	Total = total;
    }


	
    public int getProductType()
    {
    	return ProductType;
    }


	
    public void setProductType(int productType)
    {
    	ProductType = productType;
    }


	
    public int getVendorID()
    {
    	return VendorID;
    }


	
    public void setVendorID(int vendorID)
    {
    	VendorID = vendorID;
    }

}
